{
  "name": "voygen-api",
  "version": "0.1.0",
  "description": "Voygen API server for Render.com hosting",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "ts-node-esm src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "echo \"Tests not implemented yet\" && exit 0",
    "build-api-only": "npm install && tsc",
    "start-api-only": "node dist/index.js",
    "install-librechat": "npm run clone-librechat && npm run setup-librechat",
    "clone-librechat": "git clone https://github.com/danny-avila/LibreChat.git librechat",
    "setup-librechat": "cd librechat && npm ci && cp ../librechat.yaml . && cd ..",
    "build-all": "npm run build && cd librechat && npm run build && cd ..",
    "start-librechat": "cd librechat && npm start"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "body-parser": "^1.20.2",
    "jsdom": "^23.0.0",
    "@mozilla/readability": "^0.5.0",
    "node-fetch": "^3.3.2",
    "dotenv": "^16.3.1",
    "http-proxy-middleware": "^2.0.6"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "ts-node": "^10.9.1",
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.14",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.8.0",
    "@types/http-proxy-middleware": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "voygen",
    "travel",
    "api",
    "express",
    "mcp"
  ],
  "author": "Voygen Team",
  "license": "MIT"
}